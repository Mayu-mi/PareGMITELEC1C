@model Student;

<h1>Student Details</h1>

<table class="table table-dark">
    <tr>
        <th>Id</th>
        <td>@Model.StudentId</td>
    </tr>
    <tr>
        <th>Last Name</th>
        <td>@Model.StudentLastName</td>
    </tr>
    <tr>
        <th>First Name</th>
        <td>@Model.StudentFirstName</td>
    </tr>
    <tr>
        <th>Course</th>
        <td>@Model.Course</td>
    </tr>
    <tr>
        <th>Email</th>
        <td>@Model.Email</td>
    </tr>
    <tr>
        <th>Admission Date</th>
        <td>@Model.DateEnrolled</td>
    </tr>
    <tr>
        <th>GPA</th>
        <td>@Model.GPA</td>
    </tr>
</table>